-->
    	   	
    	<record id="view_tr_commision_tree" model="ir.ui.view">
    		<field name="name">view_tr_commision_tree</field>
    		<field name="model">tr.commission</field>
    		<field name="type">tree</field>
    		<field name="arch" type="xml">
    			<tree string="Commission">
    				<field name="partner_id"/>
    				<field name="name"/>
    			</tree>
    		</field>
    	</record>
    	<record id="view_tr_commission_form" model="ir.ui.view">
    		<field name="name">view_tr_commission_fomr</field>
    		<field name="model">tr.commission</field>
    		<field name="type">form</field>
    		<field name="arch" type="xml">
    			<form string="Commission" version="7.0">
    				<sheet>
    					
    					<group cols="4" colspan="2">    	    										
    						<field name="partner_id" domain="[('supplier', '=', supplier)]" context="{'search_default_supplier':1,'default_supplier':1,'default_customer':0}"/>
    						<field name="name"/>
    					</group>
	    				<field name="supplier" invisible="1"/>
	    				<field name="customer" invisible="1"/>
	    				<notebook colspan="4">
	    					<page string="Flight">
	    						<field name='ft_lines' nolabel="1"  context="{'supplier':supplier, 'customer':customer}">
	    							<form string="Airlines Lines">
	    								<group colspan="2" col="4">
	    									<field name="airline_id"  on_change="onchange_airline(airline_id, tpartner_id, start_date, end_date, context)"/>
	    									<field name="by_default" />
	    									<field name="tpartner_id" on_change="onchange_airline(airline_id, tpartner_id, start_date, end_date, context)" invisible="context.get('supplier')" required="context.get('customer')"/>
	    									<newline/>
	    									<field name="start_date" attrs="{'invisible':[('by_default','=',True)],'required':[('by_default','=',False)]}" on_change="onchange_airline(airline_id, tpartner_id, start_date, end_date, context)" />
	    									<field name="end_date" attrs="{'invisible':[('by_default','=',True)],'required':[('by_default','=',False)]}" on_change="onchange_airline(airline_id, tpartner_id, start_date, end_date, context)" />
	    								</group>
	    								<field mode="tree" name='tax_lines' nolabel="1" colspan="4" >
	    									<tree string="Airlines Lines" editable='top'>
	    										<field name="component_id"/>
	    										<field name="tac_a"  invisible="context.get('customer')"/>
	    										<field name="tac_b"  invisible="context.get('customer')"/>
	    										<field name="tac_c"  invisible="context.get('customer')"/>
	    										<field name="disc_a" invisible="context.get('supplier')"/>
	    										<field name="disc_b" invisible="context.get('supplier')"/>
	    										<field name="disc_c" invisible="context.get('supplier')"/>
	    									</tree>
	    								</field>
	    								<newline/>
	    								<group colspan="3" col="6">
	    									<label string="   " class="trab_bold_css"/> 
	    									<label string="On A" class="trab_bold_css"/> 
	    									<label string="On B" class="trab_bold_css"/> 
	    									<label string="On C" class="trab_bold_css"/> 
	    									<newline/> 
	    									<field name="tac_basic" string="Basic"/>
	    									<field name="tac_basic1" nolabel="1"/>
	    									<field name="tac_basic2" nolabel="1"/>
	    									<newline/>
	    									<field name="tac_other" string="Other"/>
	    									<field name="tac_other1" nolabel="1"/>
	    									<field name="tac_other2" nolabel="1"/>
	    									<newline/>
	    									<label for="tac_perc" string="TAC%%" invisible="context.get('customer')"/>
	    									<label for="tac_perc" string="Discount%%" invisible="context.get('supplier')"/>
	    									<field name="tac_perc" nolabel="1"/>
	    									<field name="tac_perc1" nolabel="1"/>
	    									<field name="tac_perc2" nolabel="1"/>
	    									<newline/>
	    									<label for="tac_perc" string="TAC Calculation" invisible="context.get('customer')"/>
	    									<label for="tac_perc" string="Discount Calculation" invisible="context.get('supplier')"/>
	    									<field name="tac_select" colspan="2" nolabel="1"/>
	    								</group>
	    								<newline/>
	    								<group invisible="context.get('supplier')" colspan="2" col="4">
	    									<field name="markup_perc"/>
	    									<field name="markup"/>
	    									<field name="servicechrg_perc"/>
	    									<field name="servicechrg"/>
	    									<field name="cancel_service" invisible="1"/>
	    									<field name="cancel_markup" invisible="1"/>
	    									
	    								</group>
	    								<newline/>
	    								<group name="taxon" invisible="context.get('customer')" colspan="3" col="6">
	    									<separator string="Supplier Taxes"/>
	    									<newline/>
	    									<field name="tptax_basic"/>
	    									<field name="tptax_airtax"/>
	    									<field name="tptax_tac"/>
	    									<field name="tptax_tds"/>
	    									<field name="tptax_other"/>
	    									<newline/>
	    									<field name="tptax_ids" colspan="6" widget="many2many_tags"/>
	    									<field name="cancel_charges" invisible="1"/>
	    								</group>
	    								<newline/>
	    								<group name="except" colspan="4">
	    									<separator string="Exception" attrs="{'invisible':[('by_default','=',True)]}" />
	    									<newline/>
	    									<field name="ftclass_ids" colspan="2" widget="many2many_tags" attrs="{'invisible':[('by_default','=',True)]}" string="Exception Flight Class"  />
	    									<field name="ftfare_ids"  colspan="2" widget="many2many_tags"  attrs="{'invisible':[('by_default','=',True)]}" string="Exception Flight Fare Type" />
	    									<field name="ftfrom_ids"  colspan="2" widget="many2many_tags" attrs="{'invisible':[('by_default','=',True)]}"/>
	    									<field name="ftto_ids"   colspan="2" widget="many2many_tags" attrs="{'invisible':[('by_default','=',True)]}"/>
	    								</group>
	    							</form>
	    							<tree string="Airlines Lines">
	    								<field name="airline_id"/>
	    								<field name="tpartner_id" invisible="context.get('supplier')"/>
	    								<field name="start_date"/>
	    								<field name="end_date"/>
	    								<field name="by_default" />
	    							</tree>
	    						</field>
	    					</page>
	    				</notebook>
    				</sheet>
    			</form>
    		</field>
    	</record>
